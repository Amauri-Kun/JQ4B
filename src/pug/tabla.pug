table#tblAlumnos(style="border: 1px solid green")
    thead 
        tr 
            th No Control 
            th Nombre 
            th Carrera
    tbody 
        each alumnos in [{nc:"12",alumno:"Huego",carrera:"ISC"},{nc:"13",alumno:"Paco",carrera:"MEC"},{nc:"13",alumno:"Luis",carrera:"IND"}]
            tr 
                td #{alumnos.nc}
                td #{alumnos.alumno}
                td #{alumnos.carrera}



